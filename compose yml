services:
   watchtower:
      container_name: watchtower
      restart: always
      image: containrrr/watchtower
      environment:
      - WATCHTOWER_NOTIFICATIONS=email
      - WATCHTOWER_NOTIFICATION_EMAIL_FROM=
      - WATCHTOWER_NOTIFICATION_EMAIL_TO=
      - WATCHTOWER_NOTIFICATION_EMAIL_SERVER=smtp.gmail.com
      - WATCHTOWER_NOTIFICATION_EMAIL_SERVER_PORT=587
      - WATCHTOWER_NOTIFICATION_EMAIL_SERVER_TLS_SKIP_VERIFY=yes
      - WATCHTOWER_NOTIFICATION_EMAIL_SERVER_USER=
      - WATCHTOWER_NOTIFICATION_EMAIL_SERVER_PASSWORD=
      - WATCHTOWER_NOTIFICATION_EMAIL_DELAY=2
      - TZ=America/New_York
      volumes:
      - /var/run/docker.sock:/var/run/docker.sock

# The --label-enable option below tells watchtower to ONLY update containers that have a label of com.centurylinklabs.watchtower.enable=true like below:
# labels:
# Watchtower
# - "com.centurylinklabs.watchtower.enable=true"


      command:  --debug --interval 1800 --cleanup --label-enable



version: '2'

services:
   db:
     container_name: db-site3
     image: hypriot/rpi-mysql
     volumes:
       - ./db_data:/var/lib/mysql
       - ./home/db:/home/db
       - ./sqldump:/sqldump
     restart: always
     environment:
       MYSQL_ROOT_PASSWORD: somewordpress
       MYSQL_DATABASE: wordpress
       MYSQL_USER: wordpress
       MYSQL_PASSWORD: wordpress

   wordpress:
     container_name: wordpress-site3
     depends_on:
       - db
     image: wordpress:latest
     ports:
       - 7044:80
     restart: always
     volumes:
       - /mnt/vpnusb3/Config/Wordpress3/data_volume:/var/www/html
       - /mnt/vpnusb3/Config/Wordpress3/home/wp:/home/wp
     environment:
       WORDPRESS_DB_HOST: db_site3:3306
       WORDPRESS_DB_USER: wordpress
       WORDPRESS_DB_PASSWORD: wordpress
       WORDPRESS_DB_NAME: wordpress
     links:
       - db:db_site3

