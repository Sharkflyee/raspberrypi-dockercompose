            - /mnt/vpnusb3/Config/openvpn/:/etc/openvpn/custom/
            - /mnt/vpnusb3/Config/openvpn/default.ovpn:/etc/openvpn/custom/default.ovpn
        environment:
            - OPENVPN_PROVIDER=CUSTOM
            - OPENVPN_CONFIG=default
            - OPENVPN_USERNAME=
            - OPENVPN_PASSWORD=
            - WEBPROXY_ENABLED=false
            - LOCAL_NETWORK=10.0.0.0/16
        logging:
            driver: json-file
            options:
                max-size: 10m
        ports:
            - '9091:9091'
        image: haugene/transmission-openvpn
