version: "2.1"
services:
  adguardhome:
    image: adguard/adguardhome
    container_name: adguardhome
    environment:
      - TZ=America/New_York
    volumes:
      - /srv/dev-disk-by-uuid-4AF4C8CDF4C8BC87/config/Adguard/workdir:/opt/adguardhome/work
      - /srv/dev-disk-by-uuid-4AF4C8CDF4C8BC87/config/Adguard/confdir:/opt/adguardhome/conf
    network_mode: host
    labels:
      - "com.centurylinklabs.watchtower.enable=true"
    ports:
      - 53:53/tcp
      - 53:53/udp
      - 80:80/tcp
      - 3000:3000/tcp
      - 67:67/udp
      - 68:68/tcp
      - 68:68/udp
      - 443:443/tcp
      - 853:853/tcp
    restart: unless-stopped



