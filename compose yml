version: "2"
services:
  bookstack:
    image: ghcr.io/linuxserver/bookstack:arm32v7-latest
    container_name: bookstack
    environment:
      - PUID=1000
      - PGID=100
      - APP_URL=http://10.0.0.32:6875
      - DB_HOST=bookstack_db
      - DB_USER=bookstack
      - DB_PASS=bookstackpass
      - DB_DATABASE=bookstackapp
    volumes:
      - /mnt/vpnusb3/config/Bookstack/bookstackconfig:/config
    labels:
      - "com.centurylinklabs.watchtower.enable=true"
    ports:
      - 6875:80
    restart: unless-stopped
    depends_on:
      - bookstack_db
  bookstack_db:
    image: ghcr.io/linuxserver/mariadb:arm32v7-latest
    container_name: bookstack_db
    environment:
      - PUID=1000
      - PGID=100
      - MYSQL_ROOT_PASSWORD=bookstackrootpass
      - TZ=America/New_York
      - MYSQL_DATABASE=bookstackapp
      - MYSQL_USER=bookstack
      - MYSQL_PASSWORD=bookstackpass
    ports:
      - 9309:3306
    labels:
      - "com.centurylinklabs.watchtower.enable=true"
    volumes:
      - /mnt/vpnusb3/config/Bookstack/mariadbconfig:/config
    restart: unless-stopped


